{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node", "strtok3"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"],
  "outDir": "./dist",
  "rootDir": "./src"
}
